extends ../_layout/master

block content
    h1 股票列表頁面
    
    table 
        tr 
            th 股票代號
            th 股票名稱
        //- each value in values
        each stock in stocks 
            tr 
                td 
                    a(href=`stock/${stock.stock_id}`) #{stock.stock_id}
                td #{stock.stock_name}